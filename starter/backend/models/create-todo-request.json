{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "create-todo",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 4,
        "pattern": "^(?!\\s*$).+"
        
      },
      "dueDate": {
        "type": "string",
        "pattern": "\\d{4}-\\d{2}-\\d{2}"
      }
    },
    "required": [
      "name",
      "dueDate"
    ],
    "additionalProperties": false
  }
